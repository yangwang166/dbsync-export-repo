{
    "locals": {
        "databricks_secret_cmr_for_each_var": {
            "cmr-host": {
                "@internal:env_val": "{{secrets/cmr/cmr-host}}",
                "@internal:env_var": "TF_VAR_cmr_cmr_host_var",
                "key": "cmr-host",
                "scope": "${databricks_secret_scope.databricks_secret_scope_cmr.id}",
                "string_value": "${var.cmr_cmr_host_var}"
            },
            "cmr-token": {
                "@internal:env_val": "{{secrets/cmr/cmr-token}}",
                "@internal:env_var": "TF_VAR_cmr_cmr_token_var",
                "key": "cmr-token",
                "scope": "${databricks_secret_scope.databricks_secret_scope_cmr.id}",
                "string_value": "${var.cmr_cmr_token_var}"
            },
            "cmr-workspace-id": {
                "@internal:env_val": "{{secrets/cmr/cmr-workspace-id}}",
                "@internal:env_var": "TF_VAR_cmr_cmr_workspace_id_var",
                "key": "cmr-workspace-id",
                "scope": "${databricks_secret_scope.databricks_secret_scope_cmr.id}",
                "string_value": "${var.cmr_cmr_workspace_id_var}"
            }
        }
    },
    "resource": {
        "databricks_secret": {
            "cmr_secrets": {
                "for_each": "${local.databricks_secret_cmr_for_each_var}",
                "key": "${each.value.key}",
                "scope": "${each.value.scope}",
                "string_value": "${each.value.string_value}"
            }
        }
    },
    "variable": {
        "cmr_cmr_host_var": {},
        "cmr_cmr_token_var": {},
        "cmr_cmr_workspace_id_var": {}
    }
}